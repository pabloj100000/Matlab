function buf = openDBimage(num)
%{
for i=60:60
    file = ['imk000', num2str(i), '.iml'];
    f1=fopen(file,'rb','ieee-be');
    w=1536;h=1024;
    buf=fread(f1,[w,h],'uint16');
    colormap(gray);
    imagesc(buf');
    pause(1)
end
