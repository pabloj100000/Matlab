function test
    InitScreen(0);
    Add2StimLogList();

    framesN = uint32(movieDurationSecs*screen.rate);

    objRects = ones(4,32);
    oneRect = SetRect(0,0, 32, 1)*PIXELS_PER_100_MICRONS;
    for i=1:32
        objRects(:,i) = 
    objRect = CenterRect(objRect, screen.rect);

    frame = randn(32,1);
    frameFFT = fft(frame);
    [theta, rho] = cart2pol(real(frameFFT), imag(frameFFT));
    figure(1)
    plot(frame);
    for i=1:framesN
        theta = rand(32,1)*2*pi;
        randFFT = rho.*exp(i*theta);
        newFrame = ifft(randFFT);
        figure(1);
        plot(real(newFrame));
    end
