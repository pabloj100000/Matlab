function CombineImages(index1, M1, C1, index2, M2, C2)
    folder = '/Users/jadz/Documents/Notebook/Matlab/Stimuli/Images/';
    files = dir(folder);

    im0 = imread([folder, files(index1).name]);
    im1 = imread([folder, files(index2).name]);

    oldMean0 = mean(mean(im0));
    oldMean1 = mean(mean(im1));
    
    
end

