function run081810_a()
CreateStimuliLogStart();

Wait2Start()

% start with some RF
pdStim=0;
RF('movieDurationSecs', 600, ...
   'barsWidth',16, ...
   'objContrast',1, ...
   'pdStim', pdStim, ...
   'waitFrames', 2 ...
   );

% Make sure long range connections are OK by running some
% differential/Global motion
pdStim=pdStim+1;
LowContrastObj_SxF( ...
    'objContrast',1, ...
    'objJitterPeriod',10, ...
    'presentationLength',10, ...
    'movieDurationSecs', 300, ...
    'pdStim', pdStim ...
    );

for i=1:2
    % Are the results from 072910 correct?
    pdStim=pdStim+1;
    UFlickerObj_SSF( ...
        'objContrast', .05, ...
        'backContrast', 1, ...
        'movieDurationSecs', 660, ...
        'pdStim', pdStim, ...
        'objSeed',i, ...
        'backSeed',i+1 ...
    );

    pdStim=pdStim+1;
    UFlickerObj_SSF( ...
        'objContrast', .10, ...
        'backContrast', 1, ...
        'movieDurationSecs', 660, ...
        'pdStim', pdStim, ...
        'objSeed',i, ...
        'backSeed',i+1 ...
    );

    % Trying to reproduce the results from 070910
    pdStim=pdStim+1;
    UFlickerObj_xSx( ...
        'objContrast', .05, ...
        'backContrast', 1, ...
        'movieDurationSecs', 300, ...
        'pdStim', pdStim, ...
        'objSeed',i, ...
        'backSeed',i+1 ...
    );

    pdStim=pdStim+1;
    UFlickerObj_xSx( ...
        'objContrast', .10, ...
        'backContrast', 1, ...
        'movieDurationSecs', 300, ...
        'pdStim', pdStim, ...
        'objSeed',i, ...
        'backSeed',i+1 ...
    );

    % Is background helping distinguish low contrast objects?
    pdStim=pdStim+1;
    LowContrastObj_SSF( ...
        'objContrast', .05, ...
        'backContrast', 1, ...
        'movieDurationSecs', 660, ...
        'pdStim', pdStim, ...
        'objSeed',i, ...
        'backSeed',i+1 ...
    );

    pdStim=pdStim+1;
    LowContrastObj_SSF( ...
        'objContrast', .10, ...
        'backContrast', 1, ...
        'movieDurationSecs', 660, ...
        'pdStim', pdStim, ...
        'objSeed',i, ...
        'backSeed',i+1 ...
    );
    StableObject_SSF( ...
        'pdStim', pdStim, ...
        'objSeed', 1, ...
        'backSeed', i ...
    );

end

CreateStimuliLogWrite();
end

