function run090310()
CreateStimuliLogStart();

Wait2Start()

% start with some RF
global pdStim;


% 572+440 = 1012 secs
for i=0:4
    pdStim = 0;
    RF('movieDurationSecs', 572, ...
        'barsWidth',16, ...
        'objContrast',1, ...
        'waitFrames', 1 ...
        );

    objContrast = 3*2^i/100;
    pdStim=pdStim+1;
    UFlickerObj_SSF( ...
        'objContrast', objContrast, ...
        'backContrast', 1, ...
        'movieDurationSecs', 660, ...
        'objSeed',i, ...
        'backSeed',i+1 ...
    );



end

CreateStimuliLogWrite();
end

