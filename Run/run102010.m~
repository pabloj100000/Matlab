function run102010()
CreateStimuliLogStart();

global pdStim;

Wait2Start()

% 900 * 9 = 8100

objContrast = [3 6 12 24]/100;
objSeed = 1;

pdStim = 0;                                 %#ok<NASGU>
RF( ...
    'objSeed', objSeed, ...
    'movieDurationSecs', 900, ...
    'barsWidth',16, ...
    'objContrast',1, ...
    'waitFrames', 1 ...
    );

for i=1:4

    pdStim=i;
    UFlickerObj_SSx( ...
        'objContrast', objContrast(i), ...
        'objSeed',1, ...
        'objJitterPeriod', 30, ...
        'backContrast', 1, ...
        'backReverseFreq', 1, ...
        'presentationLength', 300, ...
        'repeatBackSeq', 1, ...
        'movieDurationSecs', 1800, ...  % 6 presentations of 300 secs (3 with background and 3 without)
        'backSeed', 2 ...
        );

end

CreateStimuliLogWrite();
end

