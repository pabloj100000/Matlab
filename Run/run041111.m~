function run041111()
global pdStim screen

try
    CreateStimuliLogStart();
    
    
    % Define the rectangles
    [screenX, screenY] = Screen('WindowSize', max(Screen('Screens')));
    center1 =[screenX screenY]/2;    
    objRect1 = GetRects(192, center1);

    Wait2Start('debugging',0)
    
    pdStim = 0;
    %pause(1)
    RF( ...
        'movieDurationSecs', 1000, ...
        'barsWidth',16, ...
        'objContrast',1, ...
        'waitFrames', 1 ...
        );

    pdStim = pdStim+1;
    pause(1)
    OMS_identifier_LD('presentationLength',20, 'barsWidth', 16)
    
    % traditional Gatting Experiment switching contrast every 10s
    % BARS in background
    pdStim = pdStim+1;
    barsWidth = 8;
    backTex = GetBarsTex(768, barsWidth, screen, 1);
    pause(1)
    UflickerObj( ...
        'objContrast', [.03 .06 .12 .24 1], ...
        'rects', objRect1, ...
        'backMode', [0 0 1 0], ...
        'backTexture', backTex, ...
        'barsWidth', barsWidth, ...
        'backReverseFreq', 1, ...
        'backJitterPeriod', 10, ...
        'objJitterPeriod', 10, ...
        'presentationLength', 10, ...
        'movieDurationSecs', 1600 ...
        );

    % traditional Gatting Experiment switching contrast every 10s
    % and no background
    pdStim = pdStim+1;
    barsWidth = 8;
    backTex = GetBarsTex(768, barsWidth, screen, 1);
    pause(1)
    UflickerObj( ...
        'objContrast', [.03 .06 .12 .24 1], ...
        'rects', objRect1, ...
        'backMode', [0 0 0 1], ...
        'backTexture', backTex, ...
        'barsWidth', barsWidth, ...
        'backReverseFreq', 1, ...
        'backJitterPeriod', 10, ...
        'objJitterPeriod', 10, ...
        'presentationLength', 10, ...
        'movieDurationSecs', 1600 ...
        );

    % short Gating experiment trying BACKGROUND CHECKERS of 3 different
    % sizes
    for i=1:3
        barsWidth = 8*2^i;
        backTex = GetCheckersTex(768, barsWidth, screen, 1);
        pdStim = pdStim+1;
        pause(1)
        UflickerObj( ...
            'objContrast', .03, ...
            'rects', objRect1, ...
            'backMode', [0 0 1 0], ...
            'backTexture', backTex, ...
            'barsWidth', barsWidth, ...
            'backReverseFreq', 1, ...
            'backJitterPeriod', 10, ...
            'objJitterPeriod', 10, ...
            'presentationLength', 10, ...
            'movieDurationSecs', 200 ...
            );
    end
    
    % Probe gating as a function of object size
    barsWidth = 16;
    backTex = GetCheckersTex(768, barsWidth, screen, 1);
    for i=1:5
        pdStim = pdStim+1;
        pause(1)
        objRect1 = GetRects(96+32*i, center1);
        UflickerObj( ...
            'objContrast', .03, ...
            'rects', objRect1, ...
            'backMode', [0 0 1 0], ...
            'backTexture', backTex, ...
            'barsWidth', barsWidth, ...
            'backReverseFreq', 1, ...
            'backJitterPeriod', 10, ...
            'objJitterPeriod', 10, ...
            'presentationLength', 10, ...
            'movieDurationSecs', 200 ...
            );
    end
    
    % Probing retina healthiness by testing adaptation without background
    % and steady state
    pdStim = pdStim+1;
    pause(1)
    objRect1 = GetRects(192, center1);
    UflickerObj( ...
        'objContrast', [.12 1 .24 .06 .03], ...
        'rects', objRect1, ...
        'backMode', [0 0 0 1], ...
        'backTexture', backTex, ...
        'barsWidth', barsWidth, ...
        'backReverseFreq', 1, ...
        'backJitterPeriod', 100, ...
        'objJitterPeriod', 100, ...
        'presentationLength', 100, ...
        'movieDurationSecs', 500 ...
        );
    
    
    CreateStimuliLogWrite();

catch
    
    FinishExperiment();
    psychrethrow(psychlasterror);
end %try..catch..
end
